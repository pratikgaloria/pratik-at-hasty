{
  "name": "pratik-at-hasty",
  "version": "1.0.0",
  "private": true,
  "description": "Hasty front-end challenge",
  "homepage": "https://github.com/pratikgaloria/pratik-at-hasty",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --progress --colors",
    "lint": "eslint src/**/*.{js,jsx}",
    "prettier": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "start": "babel-node server",
    "start:dev": "webpack-dev-server --mode development --hot --progress --color --port 8080 --open",
    "test": "jest --coverage"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/tests"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/src/tests/coverage",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/src/tests/",
      "/src/App.jsx"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss|svg)$": "identity-obj-proxy",
      "^app(.*)$": "<rootDir>/src/app$1",
      "^tests(.*)$": "<rootDir>/src/tests$1"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^3.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss": "^8.2.10",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "redux-devtools-extension": "^2.13.7",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "highcharts": "^7.1.2",
    "highcharts-react-official": "^2.2.2",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-number-format": "^4.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.0.1",
    "react-table": "^6.10.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.5"
  }
}
